<idea-plugin url="https://github.com/kumorig/intellij-awesome-console">
	<name>Awesome Console (tslint-fork)</name>
	<version>0.2.1</version>
	<id>awesome.console</id>
	<vendor email="kumorig@gmail.com">...</vendor>
	<description><![CDATA[Check https://github.com/anthraxx/intellij-awesome-console for original project, all credits to anthraxx,]]></description>
 <change-notes><![CDATA[	]]></change-notes>
	<idea-version since-build="139.659"/>
	<depends>com.intellij.modules.lang</depends>
	<extensions defaultExtensionNs="com.intellij">
		<consoleFilterProvider implementation="awesome.console.AwesomeLinkFilterProvider"/>
		<projectService serviceInterface="awesome.console.config.AwesomeConsoleConfig" serviceImplementation="awesome.console.config.AwesomeConsoleConfig"/>
	</extensions>

	<application-components>
		<component>
			<implementation-class>awesome.console.config.AwesomeConsoleConfig</implementation-class>
			<interface-class>awesome.console.config.AwesomeConsoleConfig</interface-class>
		</component>
	</application-components>
</idea-plugin>
